package points;

public class ArrayTest {

	public static void main(String[] args) {

		int[] arr = new int[100];
		for (int i = 0; i < arr.length; i++) {
			arr[i] = i + 1;
		}
		
		int[] arr2 = new int[100];
		for (int i = 0; i < arr.length; i++) {
			arr[i] = i;
		}

		for (int i = 0; i < arr.length; i++) {
			System.out.print("arr[" + arr2[i] + "] = ");
			System.out.println(arr[i]);
		}

	}

}
