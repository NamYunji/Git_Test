package points;

public class ArrayTest {

	public static void main(String[] args) {


		
		int[] arr2 = new int[100];
		for (int i = 0; i < arr2.length; i++) {
			arr2[i] = i+1;
		}
	
		System.out.println();
		for (int i = 0; i < arr2.length; i++) {
			System.out.println("arr2[" + i + "] = " + arr2[i]);
			
		}
		

	}

}
